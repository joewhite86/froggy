<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%level] [%thread] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <!--<logger name="de.whitefrog" level="info" />-->
    <!--<logger name="de.whitefrog.frogr.service.Search" level="debug" />-->
    <!--<logger name="de.whitefrog.frogr.patch" level="debug" />-->

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>