<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%level] [%thread] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="de.whitefrog" level="info" />

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>